# API Keys Configuration
# Rename this file to .env and fill in your API keys

# Google Maps API Key (required for geocoding and traffic data)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenWeatherMap API Key (required for weather data)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Twilio credentials (optional - for SMS alerts)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Default city setting (optional)
DEFAULT_CITY=Mumbai